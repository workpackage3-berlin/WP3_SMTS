        <Param val="" valType="extendedCode" updates="constant" name="End Experiment"/>
        <Param val="" valType="extendedCode" updates="constant" name="End JS Experiment"/>
        <Param val="kb.clearEvents();&amp;#10;if (((same_button_pressed !== 0) &amp;&amp; (different_button_pressed !== 0))) {&amp;#10;    opacity_text = 0;&amp;#10;    opacity_cross = 1;&amp;#10;} else {&amp;#10;    if (((same_button_time !== 0) &amp;&amp; (decider_randomisation === 0))) {&amp;#10;        correct_response = true;&amp;#10;        response_accuracy_trial.push(1);&amp;#10;        psychoJS.experiment.addData(&quot;response_accuracy&quot;, &quot;Correct&quot;);&amp;#10;        opacity_text = 1;&amp;#10;        opacity_cross = 0;&amp;#10;    } else {&amp;#10;        if (((different_button_time !== 0) &amp;&amp; (decider_randomisation !== 0))) {&amp;#10;            correct_response = true;&amp;#10;            response_accuracy_trial.push(1);&amp;#10;            psychoJS.experiment.addData(&quot;response_accuracy&quot;, &quot;Correct&quot;);&amp;#10;            opacity_text = 1;&amp;#10;            opacity_cross = 0;&amp;#10;        } else {&amp;#10;            correct_response = false;&amp;#10;            response_accuracy_trial.push(0);&amp;#10;            psychoJS.experiment.addData(&quot;response_accuracy&quot;, &quot;Incorrect&quot;);&amp;#10;            opacity_text = 1;&amp;#10;            opacity_cross = 0;&amp;#10;        }&amp;#10;    }&amp;#10;}&amp;#10;if ((trials_trial.thisN === (nReps_trial - 1))) {&amp;#10;    opacity_text = 0;&amp;#10;    opacity_cross = 0;&amp;#10;}&amp;#10;console.log(&quot;-----------------&quot;);&amp;#10;console.log(&quot;we are in trial no:&quot;, trial_counter);&amp;#10;console.log(&quot;randomization:&quot;, decider_randomisation);&amp;#10;if ((repeat_trial === true)) {&amp;#10;    console.log(&quot;!!!!REPEAT!!!!&quot;);&amp;#10;}&amp;#10;if ((repeat_trial === false)) {&amp;#10;    console.log(&quot;list of results:&quot;, response_accuracy_trial);&amp;#10;}&amp;#10;" valType="extendedCode" updates="constant" name="End JS Routine"/>
        <Param val="#Create variable for accuracy (correct button presses)&amp;#10;#Squares are identical -&gt; decider_randomisation == 0 -&gt; &quot;left&quot; &amp;#10;#Squares are not identical (change of color/location) -&gt; decider_randomisation != 0 -&gt; &quot;right&quot; &amp;#10;kb.clearEvents()&amp;#10;&amp;#10;if same_button_pressed != 0 and different_button_pressed != 0: #&quot;left&quot; and &quot;right&quot; pressed: repeat trial&amp;#10;    opacity_text = 0&amp;#10;    opacity_cross = 1&amp;#10;    thisExp.addData('response_accuracy', &quot;Repeat/Checking&quot;)&amp;#10;elif same_button_time != 0 and decider_randomisation == 0: #squares identical&amp;#10;    correct_response = True&amp;#10;    response_accuracy_trial.append(1)&amp;#10;    thisExp.addData('response_accuracy', &quot;Correct&quot;)&amp;#10;    opacity_text = 1&amp;#10;    opacity_cross = 0&amp;#10;elif different_button_time != 0 and decider_randomisation != 0: #squares not identical&amp;#10;    correct_response = True&amp;#10;    response_accuracy_trial.append(1)&amp;#10;    thisExp.addData('response_accuracy', &quot;Correct&quot;)&amp;#10;    opacity_text = 1&amp;#10;    opacity_cross = 0&amp;#10;else: &amp;#10;    correct_response = False&amp;#10;    response_accuracy_trial.append(0) #wrong button was pressed&amp;#10;    thisExp.addData('response_accuracy', &quot;Incorrect&quot;)&amp;#10;    opacity_text = 1&amp;#10;    opacity_cross = 0&amp;#10;&amp;#10;if trials_trial.thisN == nReps_trial-1: #do not show text if we are at the end of practice/trials&amp;#10;    opacity_text = 0&amp;#10;    opacity_cross = 0&amp;#10;&amp;#10;print('-----------------')&amp;#10;print('we are in trial no:', trial_counter)&amp;#10;print('randomization:', decider_randomisation)&amp;#10;#print('response:', response_randomisation)&amp;#10;if repeat_trial == True: &amp;#10;    print('!!!!REPEAT!!!!')&amp;#10;if repeat_trial == False: &amp;#10;    print('list of results:', response_accuracy_trial)&amp;#10;    &amp;#10;" valType="extendedCode" updates="constant" name="End Routine"/>
